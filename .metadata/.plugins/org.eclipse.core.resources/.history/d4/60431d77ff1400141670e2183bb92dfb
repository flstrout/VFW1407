Titanium.UI.setBackgroundColor('#000');

var menuWindow = Ti.UI.createWindow({
	title: "Menu"
});

var logo = Ti.UI.createImageView({
	top: 50,
	image: "EagleLogo.bmp",
	height: 240
});

menuWindow.add(logo);
menuWindow.open();
